{
    "name": "redmoon/tinker-server",
    "description": "Tinker with your variables while working on your application.",
    "keywords": [
        "redmoon",
        "tinker-server"
    ],
    "homepage": "https://github.com/rlunar/tinker-server",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Roberto Luna Rojas",
            "email": "rluna@webdcg.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "clue/stdio-react": "^2.2",
        "illuminate/support": "5.8.*",
        "laravel/tinker": "^1.0",
        "psy/psysh": "^0.9.9",
        "react/socket": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "RedMoon\\TinkerServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedMoon\\TinkerServer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RedMoon\\TinkerServer\\TinkerServerServiceProvider"
            ],
            "aliases": {
                "TinkerServer": "RedMoon\\TinkerServer\\TinkerServerFacade"
            }
        }
    }
}
